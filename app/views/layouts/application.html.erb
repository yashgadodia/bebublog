
<!DOCTYPE html>
<html>
  <head>
    <title>Yash Gadodia's Daily Journal.</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_pack_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <script async src="https://www.googletagmanager.com/gtag/js?id=UA-167308723-1"></script>
    <script>
      window.dataLayer = window.dataLayer || [];
      function gtag(){dataLayer.push(arguments);}
      gtag('js', new Date());

      gtag('config', 'UA-167308723-1');
    </script>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>


  </head>

  <body>
   <%# navbar partial will be added to every page %>
    <%= render 'layouts/navbar'  %>
    <%# this guy is the master template, this template will be rendered first => yield %>
    <%= yield  %>
  </body>
</html>
